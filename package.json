{
   "name": "toypack-babel",
   "description": "Babel plugin for Toypack.",
   "version": "1.0.0",
   "type": "module",
   "main": "./build/index.js",
   "types": "./build/index.d.ts",
   "license": "MIT",
   "publishConfig": {
      "access": "public"
   },
   "scripts": {
      "dev": "set NODE_ENV=development&&concurrently \"npm run build:main -- --watch\" \"npm run type:dts -- --watch\" \"cd ./example&&npm run dev\"",
      "build": "set NODE_ENV=production&&concurrently \"npm run clean\" \"npm run build:main\" \"npm run build:bundle\" \"npm run type:dts\"",
      "build:main": "node ./swc/make-swcrc.js&&swc ./src -d ./build --config-file ./swc/.swcrc",
      "build:bundle": "webpack",
      "clean": "rimraf build browser coverage nyc_output",
      "type:dts": "tsc --emitDeclarationOnly",
      "type:check": "tsc --noEmit",
      "format": "prettier \"src/**/*.ts\" --write",
      "format:check": "prettier \"src/**/*.ts\" --check",
      "test": "vitest run",
      "test:watch": "vitest watch",
      "test:coverage": "vitest run --coverage",
      "cz": "cz"
   },
   "devDependencies": {
      "@ryansonshine/commitizen": "^4.2.8",
      "@ryansonshine/cz-conventional-changelog": "^3.3.4",
      "@swc/cli": "^0.1.62",
      "@swc/core": "^1.3.52",
      "@types/babel__core": "^7.20.1",
      "@types/babel__standalone": "^7.1.4",
      "@typescript-eslint/eslint-plugin": "^5.59.0",
      "@typescript-eslint/parser": "^5.59.0",
      "@vitest/coverage-v8": "^0.32.4",
      "chokidar": "^3.5.3",
      "concurrently": "^8.0.1",
      "eslint": "^8.38.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-import-resolver-typescript": "^3.5.5",
      "eslint-plugin-eslint-comments": "^3.2.0",
      "eslint-plugin-import": "^2.27.5",
      "jsdom": "^21.1.1",
      "lodash-es": "^4.17.21",
      "node-polyfill-webpack-plugin": "^2.0.1",
      "prettier": "^2.8.7",
      "rimraf": "^4.4.1",
      "ts-loader": "^9.4.3",
      "typescript": "^5.1.3",
      "vitest": "^0.33.0",
      "webpack": "^5.86.0",
      "webpack-cli": "^5.1.4"
   },
   "peerDependencies": {
      "toypack": "latest"
   },
   "files": [
      "./build",
      "./browser"
   ],
   "keywords": [
      "toypack"
   ],
   "dependencies": {
      "@babel/standalone": "^7.22.9"
   }
}
